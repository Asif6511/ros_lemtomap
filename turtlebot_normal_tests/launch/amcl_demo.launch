<launch>
  <!-- Map server -->
  <arg name="map_file" default="$(find turtlebot_normal_tests)/maps/willow-2010-02-18-0.10.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <!-- Localization -->
  <arg name="initial_pose_x" default="11.6"/> <!-- KL: these three values are read from /amcl_pose for willow garage with x,y,a 6, -16, 0 -->
  <arg name="initial_pose_y" default="40.8"/>
  <arg name="initial_pose_a" default="0"/> <!-- KL: is wrong...-->
  <include file="$(find turtlebot_normal_tests)/launch/includes/amcl.launch.xml">
    <arg name="initial_pose_x" value="$(arg initial_pose_x)"/>
    <arg name="initial_pose_y" value="$(arg initial_pose_y)"/>
    <arg name="initial_pose_a" value="$(arg initial_pose_a)"/>
  </include>

  <!-- Move base -->
  <include file="$(find turtlebot_normal_tests)/launch/includes/move_base.launch.xml"/>
</launch>
